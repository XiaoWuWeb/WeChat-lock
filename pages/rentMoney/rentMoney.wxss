.rent-title{
  font-size: 15px;
  color: #000;
  padding-left: 20px;
  margin-bottom: 10px;
}
.box{
  display: flex;
  flex-wrap: wrap;
  padding: 5px 10px;
}
.rent-box{
  width: 50%;
  box-sizing: border-box;
  float: left;
}
.rent-item{
  width: 162px;
  height: 75px;
  border-radius: 8px;
  font-size: 16px;
  color: rgb(96,96,96);
  background: #ddd;
  text-align: center;
  line-height: 75px;
  margin: 5px 0 5px 10px;

}
.rent-current{
  color: #fff;
  background-color: #32b51c;
}
.rent-btn{
  width: 305px;
  height: 35px;
  border-radius: 4px;
  font-size: 18px;
  color: #fff;
  text-align: center;
  line-height: 35px;
  background-color: #32b51c;
  margin: 58px auto 20px;
}
.rent-info{
  font-size: 12px;
  color: #909090;
  text-align: center;
}
.rent-info-a{
  color: #32b51c;
}