.rent-title{
  font-size: 15px;
  color: #000;
  padding-left: 20px;
  margin-bottom: 10px;
}
.box{
  display: flex;
  flex-wrap: wrap;
  padding: 5px 10px;
}
.rent-item{
  width: 162px;
  height: 75px;
  border-radius: 8px;
  font-size: 16px;
  color: rgb(96,96,96);
  background: #ddd;
  text-align: center;
  line-height: 75px;
  margin: 5px 0 5px 10px;

}
.rent-current{
  color: #fff;
  background-color: #32b51c;
}
.rent-btn{
  width: 305px;
  height: 35px;
  border-radius: 4px;
  font-size: 18px;
  color: #fff;
  text-align: center;
  line-height: 35px;
  background-color: #32b51c;
  margin: 58px auto 20px;
}
.rent-info{
  font-size: 12px;
  color: #909090;
  text-align: center;
}
.rent-info-a{
  color: #32b51c;
}
.add-box{
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  width: 100%;
  z-index: 99;
}
.add-person{
  flex: 1;
  height: 49px;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
  color: #000;
  background-color: #fff;
  border-top: 1px #ddd solid;
}
.stop-car{
  flex: 1;
  height: 50px;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
  color: #fff;
  background-color: #32b51c;
}


view{  
    box-sizing: border-box;  
}  
.item-box{  
    width: 700rpx;  
    margin: 0 auto;  
    padding:40rpx 0;  
}  
.items{  
    width: 100%;  
}  
.item{  
    position: relative;  
    border-top: 2rpx solid #eee;  
    height: 120rpx;  
    font-size: 18px;
    line-height: 120rpx;  
    overflow: hidden;  
}  
.item:last-child{  
    border-bottom: 2rpx solid #eee;  
}  
.inner{  
    position: absolute;  
    top:0;  
}  
.inner.txt{  
    background-color: #fff;  
    width: 100%;  
    z-index: 5;  
    padding:0 10rpx;  
    transition: left 0.2s ease-in-out;  
    white-space:nowrap;  
    overflow:hidden;  
    text-overflow:ellipsis;  
}  
.inner.del{  
    background-color: #e64340;  
    width: 180rpx;text-align: center;  
    z-index: 4;  
    right: 0;  
    color: #fff  
}  
.item-icon{  
    width: 64rpx;  
    vertical-align: middle;  
    margin-right: 16rpx  
}  
.thumb{  
    width: 200px;  
    height: 200px;  
    -webkit-overflow-scrolling: touch;  
    overflow: scroll;  
}  

.show{
  display: flex;
}
.show1{
  display: block;
}

.hidden{
  display: none;
}

.lock-img{
  display: block;
  width: 125px;
  height: 125px;
  margin: 40px auto 5px;
}
.lock-info{
  font-size: 14px;
  text-align: center;
  color: #333;
}